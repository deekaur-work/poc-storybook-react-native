{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.21",
    "@storybook/addon-knobs": "^4.0.0-alpha.21",
    "@storybook/addon-links": "^4.0.0-alpha.21",
    "@storybook/addon-storyshots": "^4.0.0-alpha.21",
    "@storybook/addons": "^4.0.0-alpha.21",
    "@storybook/react-native": "^4.0.0-alpha.21",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest-expo": "~27.0.0",
    "prop-types": "^15.6.2",
    "react-dom": "16.3.1",
    "react-native-scripts": "1.14.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      ".+\\.(png)$": "jest-static-stubs/png"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./src"],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "expo": "^27.0.1",
    "jest-static-stubs": "^0.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-navigation": "^2.14.0"
  }
}
